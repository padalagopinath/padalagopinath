#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        long long int n;
        cin>>n;
        long long int x=n;
        int f=0;
 
        for(long long  int i=2;i<=sqrt(x);i++)
        {
            if(n%i==0)
            {
                f=1;
                break;
            }
        }
    if(f==0)
    cout<<n<<endl;
    else
    {
        long long int i;
        for(i=2;i<=n;i++)
        {
            while(n%i==0)
            {
                n=n/i;
            }
        }
        cout<<i-1<<endl;
     }
    }
 
}
